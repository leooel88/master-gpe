<div class="relative top-6 flex justify-center">
    <div class="grow p-4 max-w-3xl bg-slate-800 rounded-lg border shadow-md sm:p-6">
        <h5 class="mb-3 text-base font-semibold text-gray-900 lg:text-xl dark:text-white">
            Liste des fiches de poste
        </h5>
        <ul class="my-4 space-y-3">
            {{#each fichePosteList}}
                <li>
                    <a href={{this.readLink}} class="flex items-center p-3 text-base font-bold text-gray-900 bg-gray-50 rounded-lg hover:bg-gray-100 group hover:shadow dark:bg-gray-600 dark:hover:bg-gray-500 dark:text-white">
                        {{#if this.even}}
                            <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" viewBox="0 0 20 20" fill="currentColor">
                                <path d="M9 2a1 1 0 000 2h2a1 1 0 100-2H9z" />
                                <path fill-rule="evenodd" d="M4 5a2 2 0 012-2 3 3 0 003 3h2a3 3 0 003-3 2 2 0 012 2v11a2 2 0 01-2 2H6a2 2 0 01-2-2V5zm3 4a1 1 0 000 2h.01a1 1 0 100-2H7zm3 0a1 1 0 000 2h3a1 1 0 100-2h-3zm-3 4a1 1 0 100 2h.01a1 1 0 100-2H7zm3 0a1 1 0 100 2h3a1 1 0 100-2h-3z" clip-rule="evenodd" />
                            </svg>
                        {{else}}
                            <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2">
                                <path stroke-linecap="round" stroke-linejoin="round" d="M9 5H7a2 2 0 00-2 2v12a2 2 0 002 2h10a2 2 0 002-2V7a2 2 0 00-2-2h-2M9 5a2 2 0 002 2h2a2 2 0 002-2M9 5a2 2 0 012-2h2a2 2 0 012 2m-3 7h3m-3 4h3m-6-4h.01M9 16h.01" />
                            </svg>
                        {{/if}}
                        <span class="flex-1 ml-3 whitespace-nowrap">{{this.label}}</span>
                        {{#if this.entryDate}}
                        <span class="flex-1 ml-3 whitespace-nowrap">Entr√©e  {{this.entryDate}}</span>
                        {{/if}}
                        {{#if this.endDate}}
                        <span class="flex-1 ml-3 whitespace-nowrap">Sortie  {{this.endDate}}</span>
                        {{/if}}
                        <span class="inline-flex items-center justify-center px-2 py-0.5 ml-3 text-xs font-medium text-gray-500 bg-gray-200 rounded dark:bg-gray-700 dark:text-gray-400">{{this.urgency}}</span>
                    </a>
                </li>
            {{/each}}
        </ul>
