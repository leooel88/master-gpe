<div class='flex flex-col items-center pb-10'>
<div class='w-1/3 bg-white rounded-lg border border-gray-700 shadow-md bg-gray-800'>
	<div class='flex flex-col items-center pb-10'>
		<h5
			class='mb-1 text-xl font-medium text-white'
		>{{manager.displayName}}</h5>
		<span class='text-sm text-gray-400'>{{manager.jobTitle}}</span>
		<span class='text-sm text-gray-400'>{{manager.mail}}</span>
		<span class='text-sm text-gray-400'>{{manager.businessPhones}}</span>
	</div>
</div>

<div class='w-1/3 bg-white rounded-lg border border-gray-700 shadow-md bg-gray-800'>
	<div class='flex flex-col items-center pb-10'>
		<h5
			class='mb-1 text-xl font-medium text-gray-900 dark:text-white'
		>{{user.displayName}}</h5>
		<span class='text-sm text-gray-400'>{{user.jobTitle}}</span>
		<span class='text-sm text-gray-400'>{{user.mail}}</span>
		<span class='text-sm text-gray-400'>{{user.businessPhones}}</span>
	</div>
</div>

<div class='w-full flex justify-center items-center'> 
{{#each coworker}}
<div class='w-1/3 bg-white rounded-lg border border-gray-700 shadow-md bg-gray-800'>
	<div class='flex flex-col items-center pb-10'>
		<h5
			class='mb-1 text-xl font-medium text-gray-900 dark:text-white'
		>{{displayName}}</h5>
		<span class='text-sm text-gray-400'>{jobTitle}}</span>
		<span class='text-sm text-gray-400'>{mail}}</span>
		<span class='text-sm text-gray-400'>{{businessPhones}}</span>
	</div>
</div>
{{/each}}	
</div>

<div class='w-full flex justify-center items-center'> 
{{#each directReports}}
<div class='w-1/3 bg-white rounded-lg border border-gray-700 shadow-md bg-gray-800'>
	<div class='flex flex-col items-center justify-center pb-10'>
		<h5
			class='mb-1 text-xl font-medium text-white'
		>{{displayName}}</h5>
		<span class='text-sm text-gray-400'>{{jobTitle}}</span>
		<span class='text-sm text-gray-400'>{{mail}}</span>
		<span
			class='text-sm text-gray-400'
		>{{businessPhones}}</span>
	</div>
</div>
{{/each}}	
</div>	

</div>
